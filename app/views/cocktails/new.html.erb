<%= simple_form_for(@cocktail) do |f| %>
  <%= f.input :name %>
  <%= f.input :photo %>
  <%= f.input :photo_cache, as: :hidden %>
  <%= f.submit 'Create', class:"btn btn-primary btn-grad rounded-pill"%>
  <%= link_to "List of cocktails", cocktails_path, class: "btn btn-primary btn-grad rounded-pill"  %>
  <% end %>
